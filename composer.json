{
  "name": "tlapnet/searcher",
  "description": "Searcher",
  "keywords": [
    "search"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/tlapnet/searcher",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Josef Benjač",
      "homepage": "https://github.com/josefbenjac"
    }
  ],
  "require": {
    "php": "^7.2.0",
    "nette/forms": "~3.0.0",
    "nette/utils": "~3.0.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.2.3",
    "ninjify/qa": "^0.9.0",
    "phpstan/extension-installer": "^1.0.0",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-nette": "^0.11.0",
    "phpstan/phpstan-shim": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "phpunit/phpunit": "^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Searcher\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Tlapnet\\Searcher\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
